            <li><a href="{{frontmatter.event1.facebookEvent}}">{{frontmatter.event1.title}}</a>: {{frontmatter.event1.day}}, <b>{{frontmatter.event1.dateshort}}</b>, {{frontmatter.event1.time}}</li>
            <li><a href="{{frontmatter.event2.facebookEvent}}">{{frontmatter.event2.title}}</a>: {{frontmatter.event2.day}}, <b>{{frontmatter.event2.dateshort}}</b>, {{frontmatter.event2.time}}</li>
            <li><a href="{{frontmatter.event3.facebookEvent}}">{{frontmatter.event3.title}}</a>: {{frontmatter.event3.day}}, <b>{{frontmatter.event3.dateshort}}</b>, {{frontmatter.event3.time}}</li>
